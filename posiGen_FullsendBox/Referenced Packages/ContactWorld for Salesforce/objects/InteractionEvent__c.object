    <validationRules>
        <fullName>Call_Object_Identifier_Deprecated</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(CallObjectIdentifier__c)</errorConditionFormula>
        <errorMessage>CallObjectIdentifier field is no longer used. Use IndexedCallObjectIdentifier.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Type_Validation</fullName>
        <active>true</active>
        <description>Event Type must be one of 3 valid Types</description>
        <errorConditionFormula>!ISPICKVAL(Type__c, &apos;Deliver&apos;) &amp;&amp; !ISPICKVAL(Type__c, &apos;Transfer&apos;) &amp;&amp; !ISPICKVAL(Type__c, &apos;Consult&apos;)</errorConditionFormula>
        <errorMessage>Event type must be either &apos;Deliver&apos;, &apos;Transfer&apos; or &apos;Consult&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>In_Call_Events_Must_Have_a_Timestamp</fullName>
        <active>true</active>
        <description>Only pre call events are allowed to have null timestamps</description>
        <errorConditionFormula>IsPreCallEvent__c = false &amp;&amp; ISNULL(TimeStamp__c)</errorConditionFormula>
        <errorMessage>Event must have a timestamp, unless it&apos;s a pre-call event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pre_Call_Events_Must_Have_a_RelatedTo</fullName>
        <active>true</active>
        <description>Pre call events must have a &quot;RelatedTo&quot; field or they are useless</description>
        <errorConditionFormula>IsPreCallEvent__c = true &amp;&amp; ((ISNULL(What__c) || What__c = &apos;&apos;) &amp;&amp; (ISNULL(Who__c) || Who__c = &apos;&apos;))</errorConditionFormula>
        <errorMessage>A pre-call event must have a related object</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RelatedTo_Field_Is_Deprecated</fullName>
        <active>true</active>
        <description>The RelatedTo field has been replaced by the Who and What fields.</description>
        <errorConditionFormula>!ISBLANK(RelatedTo__c)</errorConditionFormula>
        <errorMessage>The RelatedTo field is deprecated and has been replaced by the Who and What fields.</errorMessage>
    </validationRules>
