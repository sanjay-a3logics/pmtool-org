    <fields>
        <fullName>Average_Score__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>If( Team__r.RecordType.DeveloperName = &apos;Targeted&apos;,
      If( OR(ISBLANK(Team__r.Start_Date_Time__c),Team__r.Start_Date_Time__c &gt; NOW()),null, 
           if( Team__r.End_Date_Time__c &gt; NOW(),  Score__c / (NOW() - Team__r.Start_Date_Time__c), Score__c / ( Team__r.End_Date_Time__c -  Team__r.Start_Date_Time__c ))),null)</formula>
        <label>Daily Avg. Experience</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
