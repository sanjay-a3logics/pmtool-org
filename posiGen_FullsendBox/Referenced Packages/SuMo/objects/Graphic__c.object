    <fields>
        <fullName>Background_Colour__c</fullName>
        <deprecated>false</deprecated>
        <description>Background colour used in the tiles containing an image across the app</description>
        <externalId>false</externalId>
        <inlineHelpText>Graphics images will be place on coloured tiles if a colour is defined using a six digit hexadecimal format (e.g. #FFCC00)</inlineHelpText>
        <label>Background Colour</label>
        <length>255</length>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CloudApps_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <externalId>true</externalId>
        <label>Content ID</label>
        <length>80</length>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Image_URL__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Paste a valid image URL. Images should be a minimum of 60x60 pixels to ensure adequate resolution (larger sizes will take longer to display). Images should also be square to avoid image distortion</inlineHelpText>
        <label>Image URL</label>
        <length>255</length>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Image__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IMAGE( Image_URL__c , &quot;Image&quot;)</formula>
        <label>Image</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Type__c</fullName>
        <deprecated>false</deprecated>
        <description>Icons are used for the standard elements across the app, whilst badges are used to define badge types (used on achievements when specifying rewards)</description>
        <externalId>false</externalId>
        <inlineHelpText>Specifying a graphical element &apos;type&apos; will ensure that it is only displayed in the relevant place</inlineHelpText>
        <label>Type</label>
        <picklist>
            <picklistValues>
                <fullName>Badge</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Icon</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <label>Graphic</label>
