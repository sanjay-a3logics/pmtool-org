        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>AND( OR(ISBLANK(Publish_Date__c), Publish_Date__c &lt;= TODAY()), OR(ISBLANK(Retire_Date__c), Retire_Date__c &gt;= TODAY()), Active__c = TRUE)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Available</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
