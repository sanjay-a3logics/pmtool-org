    </fields>
    <fields>
        <fullName>Complexity_Text__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( Complexity__c = 1,  $Label.easy,
  IF(Complexity__c = 2,  $Label.average,
    IF(Complexity__c = 3, $Label.difficult,
       IF(Complexity__c = 4, $Label.very_hard,
          &quot;Undefined&quot;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Complexity</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
