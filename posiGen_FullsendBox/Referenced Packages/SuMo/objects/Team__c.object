    </validationRules>
    <validationRules>
        <fullName>Start_date_must_be_before_end_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK(End_Date_Time__c)), NOT(ISBLANK( Start_Date_Time__c )),   End_Date_Time__c &lt;  Start_Date_Time__c )</errorConditionFormula>
        <errorMessage>The start date must be before the end date</errorMessage>
    </validationRules>
