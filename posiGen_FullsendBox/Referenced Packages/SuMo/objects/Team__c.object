    <validationRules>
        <fullName>Chatter_notification_requires_group</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
(Notify_by_Chatter__c == TRUE),
ISBLANK( ChatterGroupId__c)
)</errorConditionFormula>
