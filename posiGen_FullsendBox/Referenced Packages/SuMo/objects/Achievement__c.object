var records = {!GETRECORDIDS($ObjectType.Achievement__c)}; 
var url = parent.location.href;
if (records[0] == null) { 
   alert(&quot;Please select at least one Achievement&quot;);     
} 
else { 

   var recordIds = [];
   for (var a=0; a &lt; records.length; a++) {
      recordIds.push(&quot;&apos;&quot; + records[a] + &quot;&apos;&quot;);
   }

   var q = &quot;Select Id, Name, success__Active__c FROM success__Achievement__c WHERE Id IN (&quot; + recordIds.join(&apos;,&apos;) + &quot;)&quot;;

   var result = sforce.connection.query(q);

   var resultRecords = result.getArray(&quot;records&quot;);

   for (var a=0; a &lt; resultRecords.length; a++) {
       resultRecords[a].success__Active__c = !(resultRecords[a].success__Active__c == &apos;true&apos;);
   }

   result = sforce.connection.update(resultRecords);
       
   var hasErrors = false;
   var errorReport = &quot;The following Achievements could not be updated:\n\n&quot;;
   for (var j = 0; j &lt; result.length; j++) {
          if (!result[j].getBoolean(&quot;success&quot;)) {
            hasErrors = true;
            errorReport += (resultRecords[j].Name + &quot;, error: &quot; + result[j].errors.message + &quot;\n\n&quot;);
            if (j == 10 &amp;&amp; j &lt; result.length) {
              //errorReport += (&quot;Maximum errors to display.  Will show first 10 out of &quot; + &quot;result.length\n&quot;);
              break;
            }
          }
        }
 
   if (hasErrors) {
     alert(errorReport);
   }
   
   try {
     if (typeof ListViewport !== &apos;undefined&apos;
          &amp;&amp; ListViewport.instances !== &apos;undefined&apos;
     ) {
       for (var lvinstance in ListViewport.instances) {
           ListViewport.instances[lvinstance].refreshList()
       }
     }
   } catch (e) {
     parent.location.href = url;
   }
}</url>
