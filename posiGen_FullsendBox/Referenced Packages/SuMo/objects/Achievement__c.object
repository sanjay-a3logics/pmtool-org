    <validationRules>
        <fullName>If_Not_Aggregate_then_no_Aggregate_value</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Aggregate_Field__c ),NOT(ISBLANK( Number_of_occurrences__c )), ISBLANK( Number_of_occurrences__c ))</errorConditionFormula>
        <errorMessage>If aggregate rewarding is required, an Aggregate Function and a Function Value must both be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Number_of_occurrences_then_aggregate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(   Repetition_Limit__c   )) &amp;&amp; ISBLANK(  Aggregate_Field__c)</errorConditionFormula>
        <errorMessage>If aggregate rewarding is required, an Aggregate Function and a Function Value must both be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Time_Limit_then_Unit_of_Time</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(  Time_limit_in_days__c  ),NOT(ISPICKVAL( Time_Limit_Unit__c , &apos;&apos;)), ISPICKVAL( Time_Limit_Unit__c , &apos;&apos;))</errorConditionFormula>
        <errorMessage>If a Time Limit is required, then both a value and a period must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Wait_Time_then_Unit_of_Time</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Minimum_hours_between_actions__c ), NOT(ISPICKVAL(Minimum_time_unit__c, &apos;&apos;)), ISPICKVAL(Minimum_time_unit__c, &apos;&apos;))</errorConditionFormula>
        <errorMessage>If a Wait Time is required, then both a value and a period must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_not_exceed_998</fullName>
        <active>true</active>
        <errorConditionFormula>Maximum_number_of_rewards__c &gt; 998</errorConditionFormula>
        <errorMessage>The maximum number of rewards that can be set is  998</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>badge_name_and_type_are_dependent</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(ISBLANK(Badge_Name__c), NOT(ISBLANK( Badge_Type__c ))), AND(ISBLANK(Badge_Type__c), NOT(ISBLANK( Badge_Name__c ))))</errorConditionFormula>
        <errorMessage>Badge name and Badge Type fields must either both be populated or both be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>reward_message_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Reward_Message__c)</errorConditionFormula>
        <errorDisplayField>Reward_Message__c</errorDisplayField>
        <errorMessage>Required Field</errorMessage>
    </validationRules>
