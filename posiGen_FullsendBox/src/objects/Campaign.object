    <validationRules>
        <fullName>Referral_Past_Due_Restricted</fullName>
        <active>false</active>
        <description>Referral can&apos;t be paid out if referrer is past due.</description>
        <errorConditionFormula>AND(Referrer_Past_Due_Amount__c  &gt; 0, Add_to_Next_Payout_Batch__c  = TRUE)</errorConditionFormula>
        <errorDisplayField>Add_to_Next_Payout_Batch__c</errorDisplayField>
        <errorMessage>You can not pay referrers who are past due!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referrals_Problem_outside_of_sales</fullName>
        <active>false</active>
        <description>stop the automatic creation of referrals from operations updates</description>
        <errorConditionFormula>NOT(OR($UserRole.Id =&quot;00Ed0000000csQs&quot;,
$UserRole.Id =&quot;00Ed0000000dLZL&quot;,
$UserRole.Id=&quot;00Ed0000000cjd4&quot;,
$UserRole.Id=&quot;00Ed0000000cr5e&quot;, 
$User.Id =&quot;005d0000001blPP&quot;,
$User.Id = &quot;005d0000002t0XF&quot;, 
$User.Id = &quot;005d0000003ZZK3&quot;, 
$User.Id = &quot;005d0000003jj7o&quot;, 
$User.Id =&quot;005d0000003XKcb&quot;, 
$User.Id =&quot;005d0000004Zfpq&quot;,  
$User.ProfileId = &quot;00ed0000000WxCH&quot;,
$UserRole.Id = &quot;00Ed0000000eNRr&quot;,
$User.ProfileId = &quot;00ed0000000X2nD&quot;, 
$User.Alias = &quot;pnet&quot;))</errorConditionFormula>
        <errorMessage>Issue with Referrals. Please email sfischer@posigen.com with the url of the record you were updating and state the field you need updated with it&apos;s value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Referrals_for_SysAdmin</fullName>
        <active>false</active>
        <description>stop the automatic creation of referrals from sysadmin data loads</description>
        <errorConditionFormula>OR( OwnerId = &quot;005d0000003YDjT&quot;, OwnerId = &quot;005d0000003XKcb&quot;)</errorConditionFormula>
        <errorMessage>Validation Rule: Restrict_Referrals_for_SysAdmin</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SRT_Referral_Earned_Restriction</fullName>
        <active>false</active>
        <errorConditionFormula>AND(SRT_Referral_Earned__c  = FALSE, Add_to_Next_Payout_Batch__c =TRUE)</errorConditionFormula>
        <errorDisplayField>Add_to_Next_Payout_Batch__c</errorDisplayField>
        <errorMessage>You can not add a referral to a payout batch if it is not earned!</errorMessage>
    </validationRules>
