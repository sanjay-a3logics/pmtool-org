    <fields>
        <fullName>Ready_To_Work__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
	NOT(TEXT(Status__c) = &apos;Completed&apos;),
	OR( NOT(Milestone__r.Has_Predecessor__c),  Milestone__r.Predecessor_Milestone_Complete__c ),
	OR( NOT(Has_Predecessor__c), 
		AND(Predecessor_Task__r.Complete__c,
			OR(NOT( Predecessor_Task__r.Task_Is_a_Decision__c ), 
			   OR(ISPICKVAL(Successor_Applies_When_Pred_Decision_Is__c, &apos;Either&apos;), 
			      TEXT(Successor_Applies_When_Pred_Decision_Is__c) = TEXT(Predecessor_Task__r.Decision_Answer__c))
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ready To Work</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
