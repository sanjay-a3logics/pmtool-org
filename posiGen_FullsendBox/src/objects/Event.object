    <validationRules>
        <fullName>Appt_Status_wout_Sign_Status</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type , &apos;Sales Appointment&apos;) 
&amp;&amp;
TEXT( Appointment_Kept__c )=&apos;Yes&apos;
 &amp;&amp; 
 ISPICKVAL( Client_Signed__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Client_Signed__c</errorDisplayField>
        <errorMessage>You must update whether or not the client signed!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_Reason_For_Appointment_Not_Kept</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type , &apos;Sales Appointment&apos;) 
&amp;&amp; 
TEXT( Appointment_Kept__c ) = &apos;No&apos; &amp;&amp; ISPICKVAL(  Not_Kept_Reason__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Not_Kept_Reason__c</errorDisplayField>
        <errorMessage>A reason must be provided if the appointment is not kept.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_Reason_for_not_Signing</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type , &apos;Sales Appointment&apos;) 
 &amp;&amp; 
TEXT( Client_Signed__c )  = &apos;No&apos; &amp;&amp;   ISPICKVAL( Reason_for_not_signing__c , &apos;&apos;)</errorConditionFormula>
        <errorMessage>You must choose a reason why the client did not sign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Reason_Not_Kept</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(  Not_Kept_Reason__c  , &quot;Other&quot;)  &amp;&amp;  ISBLANK(  Other_Reason_Not_Kept__c  )</errorConditionFormula>
        <errorMessage>You must specify why the appointment was not kept.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_reason_client_did_not_sign</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Reason_for_not_signing__c , &quot;Other&quot;)  &amp;&amp;  ISBLANK( Other_reason__c )</errorConditionFormula>
        <errorMessage>You must provide a reason why the client did not sign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Is_Required</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(TEXT(Type))</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>Type is required!</errorMessage>
    </validationRules>
