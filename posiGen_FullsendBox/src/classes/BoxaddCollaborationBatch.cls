public class BoxaddCollaborationBatch implements Database.Batchable<Sobject>,Database.AllowsCallouts, Database.Stateful{
    public String base_url = 'https://posigen.app.box.com/files/0/f/';
    public Integer record_count = 0; 
    public Integer record_count_stop = 0;
    public string folderName ;
    public string UserEmail;  
    public String ProfileId;
    public List<Sub_Folder__c> folderList;
    public List<Box_Error__c> bxErrList;
   
  public BoxaddCollaborationBatch(String folderName,Integer record_count_stop,String ProfileId,String UserEmail){
    this.record_count_stop = record_count_stop;
    this.folderName = folderName;
    this.UserEmail = UserEmail;
    this.ProfileId = ProfileId;
  } 
    
    public Database.QueryLocator start(Database.BatchableContext BC){
      Set<String>stageSet = new Set<String>();
          for(BoxAccessMatrix_Stage__c bstge:[select id, Box_Permission__c,BoxAccessMatrix_Profile__r.BoxAccessMatrix_SubFolder__r.SubFolder_Name__c,BoxAccessMatrix_Profile__r.Lookup_field_Name__c,BoxAccessMatrix_Profile__r.Profile_Name__c, BoxAccessMatrix_Profile__r.Profile_Id__c, Stage__c,check__c from BoxAccessMatrix_Stage__c where BoxAccessMatrix_Profile__r.Profile_Id__c=:ProfileId and check__c=true]){
               stageSet.add(bstge.Stage__c);
            }
        return Database.getQueryLocator('Select Id,Name,CF_Deal__c,CF_Box_Folder_Id__c,CF_Deal__r.Name from Sub_Folder__c where Name=:folderName and CF_Deal__c!=null and CF_Deal__r.Opp_Pipeline_Stage__c in:stageSet');
    }
    
    public void execute(Database.BatchableContext BC, List<Sub_Folder__c> folderList){
        bxErrList = new List<Box_Error__c>();
        List<Collaboration__c> lstcollab = new List<Collaboration__c>();
       try{
         
           system.debug('######'+folderList.size());
            Box__c box = Box__c.getInstance();             
            
            for(Sub_Folder__c  fld : folderList){
            
                 system.debug('=========='+UserEmail);                     
                HttpRequest req = new HttpRequest();
                req.setEndpoint('https://api.box.com/2.0/folders/'+fld.CF_Box_Folder_Id__c+'/'+'collaborations');
                req.setMethod('GET');
                req.setHeader('Authorization','Bearer '+box.BoxAccess_Token__c);
                Http httpName = new Http();
                HTTPResponse res = httpName.send(req);
                system.debug('UpdateName'+res+res.getbody());  
                FolderWrapper fw = (FolderWrapper) System.JSON.deserialize(res.getBody(), FolderWrapper.class);
                if(res.getStatusCode() == 200){
                   system.debug('======'+fw.entries);
                for(cls_entries enn:fw.entries){    
                   if(enn.status=='accepted'&& enn.accessible_by!=null){
                       system.debug('------'+enn.id+'-------'+fld.CF_Box_Folder_Id__c+'---------'+UserEmail+'-----'+enn.accessible_by.login);
                      if(enn.accessible_by.login==UserEmail){
                       
                        Collaboration__c clb = new Collaboration__c();
                        clb.BoxCollaboration_Id__c = enn.id;
                        clb.Role__c= enn.role;
                        clb.BoxFolder_Id__c= fld.CF_Box_Folder_Id__c;
                        clb.BoxUser_Id__c='';
                        clb.SfUser_email__c=enn.accessible_by.login;
                        clb.SfProfile_Id__c ='00ed0000000WgyUAAS';
                        lstcollab.add(clb);
                        }
                       }
                      
                      }
                     }  
                 else{
                     Box_Error__c bxerr = new Box_Error__c();
                     bxerr.Deal__c = fld.CF_Deal__c; 
                     bxerr.name = String.valueOf(res.getStatusCode()); 
                     bxerr.Message__c = ''; 
                     bxerr.Body__c = res.getBody();
                     bxerr.Type__c = 'Not add Collaboration Name';
                     bxerr.Not_Uploaded_Id__c=''+fld.CF_Box_Folder_Id__c;
                     bxerr.Status__c = 'Error';
                     bxErrList.add(bxerr);
                   }
              
                
              
              }
            
            record_count += folderList.size();
            if(record_count == record_count_stop){
           system.debug('######################################## job aborted record_count is'+ record_count);
           System.abortJob(BC.getJobId());
            }
    
        }
        catch(Exception e){
            System.debug(' ***Error '+e.getMessage());
             Box_Error__c bxerr = new Box_Error__c();
             bxerr.name = 'Exception';
             bxerr.Status__c = 'Error';
             bxerr.Type__c = 'Not add Collaboration Name'; 
             bxerr.Message__c = e.getMessage(); 
             bxerr.Body__c = e.getStackTraceString();
             
             bxErrList.add(bxerr);
        }
       finally{
          insert lstcollab;
          insert bxErrList;
          } 
    }
    
    
  public void finish(Database.BatchableContext BC){}  
  
  
